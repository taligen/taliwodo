Alias ${appconfig.context}/css/ ${package.codedir}/css/

WSGIScriptAlias ${appconfig.context} ${package.codedir}/web/index.py

WSGIDaemonProcess ${package.name}-${appconfig.appconfigid} processes=2 threads=10 \
       umask=0007 inactivity-timeout=900 maximum-requests=1000 \
       python-path=${package.codedir}/web:${appconfig.datadir}:/usr/lib/python3.6/site-packages/
WSGIProcessGroup ${package.name}-${appconfig.appconfigid}

<Directory "${package.codedir}">
    Require all granted
</Directory>
<Directory "${appconfig.datadir}">
    Require all granted
</Directory>
